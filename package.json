{
  "name": "media-geolocation-notification",
  "version": "1.0.0",
  "description": "![CI](https://github.com/DmitriyIskra/chat-frontend/actions/workflows/web.yml/badge.svg)",
  "main": "webpack.config.js",
  "scripts": {
    "installWebPackNpm": "npm i --save-dev webpack webpack-cli html-webpack-plugin html-loader mini-css-extract-plugin css-loader babel-loader @babel/preset-env",
    "installDevServerNpm": "npm i --save-dev webpack-dev-server",
    "installEslintNpm": "npm i --save-dev eslint eslint-plugin-import eslint-config-airbnb-base",
    "installJestNpm": "npm i --save-dev jest @babel/core @babel/cli @babel/preset-env",
    "installWebpackYarn": "yarn add webpack webpack-cli html-webpack-plugin html-loader mini-css-extract-plugin css-loader babel-loader --dev",
    "installDevserverYarn": "yarn add webpack-dev-server --dev",
    "installJestYarn": "yarn add jest @babel/core @babel/cli @babel/preset-env --dev",
    "installCoreJsYarn": "yarn add core-js@3",
    "installEslintYarn": "yarn add eslint eslint-plugin-import eslint-config-airbnb-base --dev",
    "installPuppeteerYarn": "yarn add puppeteer@19.4.0 --dev",
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint .",
    "test": "jest",
    "e2e": "jest --testPathPattern=./e2e/ --runInBand",
    "coverage": "jest --coverage",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "core-js": "^3.31.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43"
  }
}
